{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "extends": "airbnb",
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "no-param-reassign": "off",
        "indent": [
            "error",
            2
        ],
        "no-console": 0,
        "no-unused-expressions": "off",
        "camelcase": "off",
        "radix": "off",
        "no-underscore-dangle": "off",
        "arrow-parens": [
            "error",
            "as-needed"
        ],
        "new-cap": [
            "error",
            {
                "capIsNew": false
            }
        ]
    }
}